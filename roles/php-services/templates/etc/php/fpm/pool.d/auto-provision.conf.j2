[auto-provision]
user = {{ web_user }} 
group = {{ web_groupname }} 
listen = /var/run/php/php{{ php_version }}-fpm-auto-provision.sock
listen.owner = {{ web_user }} 
listen.group = {{ web_groupname }}
listen.mode = 0666
pm = dynamic
pm.max_children = 16
pm.start_servers = 2
pm.min_spare_servers = 2
pm.max_spare_servers = 2
pm.process_idle_timeout = 10s
pm.max_requests = 32
slowlog = /var/log/php-fpm-slow.log
php_admin_value[memory_limit] = 256M
env[TMP] = /var/tmp
env[TMPDIR] = /var/tmp
env[TEMP] = /var/tmp
env[EP_DB_HOST] = "{{ auto_provision_db_host }}"
env[EP_PROV_DIR] = "{{ eqpress_root_dir }}/provision"
env[EP_PROV_LOG] = "{{ auto_provision_log_file }}"
env[EP_DB_CHARSET] = "{{ auto_provision_db_charset }}"
env[EP_DB_COLLATE] = "{{ auto_provision_db_collate }}"
env[EP_DEV_RAND] = "{{ auto_provision_dev_rand }}"
env[CONSOLE_API_KEY_SECRET] = "{{ console_secret_key }}"
