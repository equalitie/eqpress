---
org_name: "{{ org_name }}"
org_support_email: "{{ org_support_email }}"
org_support_url: "{{ org_support_url }}"
monitoring_email: "{{ monitoring_email }}"

# SSL/TLS
root_cert_country: "{{ root_cert_country }}"
root_cert_state: "{{ root_cert_state }}"
root_cert_city: "{{ root_cert_city }}"
root_cert_org_unit: "{{ root_cert_org_unit }}"
root_cert_common_name: "{{ root_cert_common_name }}"
root_cert_email: "{{ root_cert_email }}"

# ssh - defined in local role defaults
ssh_known_hosts_command: "{{ ssh_known_hosts_command }}" 
ssh_known_hosts_file: "{{ ssh_known_hosts_file }}" 

# mail service
mandrill_password: "{{ mandrill_password }}"
mandrill_username: "{{ mandrill_username }}"
sendgrid_password: "{{ sendgrid_password }}"
sendgrid_username: "{{ sendgrid_username }}"
eqpress_mail_username: "{{ eqpress_mail_username }}"
eqpress_mail_password: "{{ eqpress_mail_password }}"
eqpress_mail_server: "{{ eqpress_mail_server }}"
mail_service: "{{ email_service }}"
email_domain: "{{ email_domain }}"

# misc
timezone: "Etc/UTC"
console_secret_key: '{{ random_password1 }}'
rampass_domain: "{{ rampass_domain }}"
rampass_url: "https://{{ rampass_domain }}"
eqpress_platform: "3.0"
method_allowedges: true
edgespermit_key: "{{ edgespermit_key }}"
edgespermit_min_edges: {{ edgespermit_min_edge }}
